/* page/successCheckIn/successCheckIn.wxss */
.wrapper{
  padding: 60rpx 40rpx;
}
.cardView{
  /* border-radius: 6px; */
   /* box-shadow: 0px 0px 5px 0px grey;  */
}
.topSection{
  padding: 30rpx 95rpx;
  background: #7d52f8;
  display: flex;
  border-top-left-radius: 6px; 
  border-top-right-radius: 6px; 
  /* box-shadow: 0px -1px 2px 0px #7d52f8;   */
}
.roomView{
  width: 50%;
  padding: 16rpx 0;
  box-sizing: border-box;
  border-right: 1px #bea8fb solid;
}
.peoName{
  width: 50%;
  padding: 16rpx 0;
  box-sizing: border-box;
  padding-left: 75rpx;
}
.sFont{
  font-size: 26rpx;
  color: #bea8fb;
  margin-bottom: 15rpx;
}
.bFont{
  font-size: 36rpx;
  color: #ffffff;
}
.bottomSection{
   box-shadow: 0px 15px 20px 3px #dedede; 
  border-bottom-left-radius: 6px;
  border-bottom-right-radius: 6px;
}
.roomType{
  padding: 32rpx 42rpx;
  font-size: 30rpx;
  color: #666666;
}
.iconBox{
  display: flex;
  overflow: hidden;
  justify-content: space-between;
  align-items: center;
}
.leftCircle{
  width:  12px;
  height: 12px;
  background: #e3e3e3;
  border-radius: 50%;
  margin-left: -6px;
}
.midDashed{
  flex:1;
  border-top: 1px #eaedf1 dashed;
}
.rightCircle{
  width:  12px;
  height: 12px;
  background: #e3e3e3;
  border-radius: 50%;
  margin-right: -6px;
}
.dateTime{
  display: flex;
  font-size: 30rpx;
  color: #666666;
  padding: 32rpx 42rpx;
  justify-content: space-between;
}
.iconView{
  padding: 90rpx 60rpx 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 36rpx;
  color: #666666;
}
.iconCard{
  width: 190rpx;
  height: 168rpx;
  background: url("data:image/png;base64,") no-repeat;
   background-size: 100% 100%;
}
.btnBox{
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
}