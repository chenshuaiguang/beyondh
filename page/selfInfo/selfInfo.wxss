/* page/clientInfo/clientInfo.wxss */
.wrapper{
  padding: 60rpx 40rpx;
}
.infoView{
  /* width: 100px; */
  /* height: 100px; */
  border-radius: 6px;
  box-shadow: 0px 0px 5px 0px grey; 
  padding: 0 20rpx;
  font-size: 30rpx;
  color: #666666;
}
.infoItem{
  display: flex; 
  padding: 60rpx 20rpx;
  align-items: center; 
}
.headIcon{
  width: 42px;
  height: 42px;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
  margin-right: 35rpx;
}
.inputBox{
  flex:1;
}
.peopleName{
  font-size: 36rpx;
  color: black;
}
.phoneNum{
  display:flex;
  align-items:center;
  justify-content:space-between;
  font-size: 36rpx;
  color: black;
}
.phoneIcon{
  width: 42px;
  height: 42px;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  margin-right: 35rpx;
}
.idTypeIcon{
  width: 42px;
  height: 42px;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  margin-right: 35rpx;
}
.idType{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.idTypeName{
  color: black;
  font-size: 36rpx;
}
.grayNextIcon{
  width: 10px;
  height: 10px;
  border-top: 2px #d4d4d4 solid;
  border-right: 2px #d4d4d4 solid;
   transform: rotate(45deg); 
}
.idIcon{
  width: 42px;
  height: 42px;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  margin-right: 35rpx;
}
.borderItem{
  border-top: 1px #e5e5e5 solid;
}
.attenView{
  padding-top: 32rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.attendIcon{
  width: 12px;
  height: 12px;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  margin-right: 11rpx;
}
.bottomView{
  padding: 80rpx 20rpx 93rpx 20rpx;
  display: flex;
  justify-content: space-between;
}