/* page/mine/mine.wxss */
.wrapper{
  padding: 40rpx;
  font-size: 36rpx;
}
.topView{
  padding: 0 18rpx 36rpx;
  display: flex;
  align-items: center;
}
.left{
  width: 78px;
  height: 78px;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.right{
  margin-left: 40rpx;
}
.reviseBtn[type='default']{
  font-size: 30rpx;
  line-height: 48rpx;
  margin-top: 34rpx;
  box-shadow: none;
}
.itemsView{
  padding: 0 20rpx 200rpx;
  box-shadow: 0px 2px 10px 0px grey;
  border-radius: 6px;
}
.lists{
  padding: 60rpx 40rpx;
  border-bottom: 1px #e5e5e5 solid;
  display:flex;
  align-items: center;
}
.orderIcon{
  width: 42px;
  height: 42px;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.contactIcon{
  width: 42px;
  height: 42px;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.invoiceIcon{
  width: 42px;
  height: 42px;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
}
.info{
  margin-left: 38rpx;
  flex: 1;
}
.grayColor{
  color: #9b9b9b;
  font-size: 30rpx;
}
.flexView{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.grayIcon{
  width: 10px;
  height: 10px;
  border-top: 2px #d4d4d4 solid;
  border-right: 2px #d4d4d4 solid;
  transform: rotate(45deg);
}

