/* page/clientList/clientList.wxss */
.wrapper{
  font-size: 36rpx;
}
.grayBg{
  background: #f9f9f9;
}
.items{
  border-bottom: 1px #e5e5e5 solid;
}
.showItem{
  padding: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.upIcon{
  width: 10px;
  height: 10px;
  border-top: 1px #9b9b9b solid;
  border-right: 1px #9b9b9b solid;
  transform: rotate(-45deg);
  margin-left: 33rpx;
}
.downIcon{
  width: 10px;
  height: 10px;
  border-top: 1px #9b9b9b solid;
  border-right: 1px #9b9b9b solid;
  transform: rotate(135deg);
  margin-left: 33rpx;
}
.left{
  display: flex;
  align-items: center;
}
.right{
  display: flex;
  align-items: center;
}
checkbox .wx-checkbox-input{
  width: 18px;
  height: 18px;
  border-radius: 50%; 
  margin-right: 60rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
  background: #ff4242;
  border: 1px #ff4242 solid;
}
.hideItem{
  padding: 0 20rpx;
}
.hideItemView{
  padding: 40rpx 60rpx;
  border-top: 1px #e5e5e5 solid;
  font-size: 30rpx;
  line-height: 50rpx;
}
.addView{
  display: flex;
  padding: 40rpx 80rpx;
  align-items: center;
  color: #916cfb;
  font-size: 30rpx;
}
.addIcon{
  width: 15px;
  height: 15px;
  margin-right: 15rpx;
  background:  url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
}
.btnBox{
  padding: 100rpx 38rpx;
}