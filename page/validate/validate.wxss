/* page/validate/validate.wxss */
.tel_img {
    margin:10px 0 20px;
    width:100%;
    padding-top:40%;
    background:url("data:image/png;base64,") center no-repeat;
    background-size:contain;
}
.flex {
    display:flex;
    display:-webkit-flex;
}
.flex-1 {
    flex:1;
    -webkit-flex:1;
}
.form-wrap {
    margin:0 15% 20px;
    font-size:15px;
}
.form-wrap text {
    font-weight:bold;
    color:#444;
    font-family:'微软雅黑';
}
.input-box {
    padding:15px 0;
}
.tel_num_line {
    position:relative;
}
.tel_num_line:after {
    content:"";
    position:absolute;
    width:110%;
    bottom:0;
    left:-5%;
    border-bottom:1px solid #f0f0f0;
}
.v_code , .input_tit{
    justify-content:space-between
}
.v_code input {
    border:1px solid #ddd;
    display:inline-block;
    width:40px;
    height:40px;
}
.input_tit .send {
    margin-right:15px;
    padding:2px 20px;
    font-weight:normal;
    font-size:.8em;
    color:#7D52F8;
    border:1px solid;
    border-radius:15px;
    background-color:#fff;
}

